<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/result_frame_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ResultFragment">

    <TextView
        android:id="@+id/tv_num"
        style="@style/FragmentTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_num"
        app:layout_constraintBottom_toTopOf="@+id/tv_dob"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_dob"
        style="@style/FragmentTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_dob"
        app:layout_constraintBottom_toTopOf="@+id/tv_gender"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_num" />

    <TextView
        android:id="@+id/tv_gender"
        style="@style/FragmentTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_gender"
        app:layout_constraintBottom_toTopOf="@+id/tv_province"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_dob" />

    <TextView
        android:id="@+id/tv_province"
        style="@style/FragmentTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_province"
        app:layout_constraintBottom_toTopOf="@+id/tv_civil"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_gender" />

    <TextView
        android:id="@+id/tv_civil"
        style="@style/FragmentTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_civil_status"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_province" />

    <TextView
        android:id="@+id/tv_num2"
        style="@style/FragmentTextView.Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_data"
        app:layout_constraintBottom_toTopOf="@+id/tv_dob2"
        app:layout_constraintEnd_toEndOf="@+id/tv_dob2"
        app:layout_constraintStart_toEndOf="@+id/tv_num"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_dob2"
        style="@style/FragmentTextView.Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_data"
        app:layout_constraintBottom_toTopOf="@+id/tv_gender2"
        app:layout_constraintEnd_toEndOf="@+id/tv_gender2"
        app:layout_constraintStart_toEndOf="@+id/tv_dob"
        app:layout_constraintTop_toBottomOf="@+id/tv_num2" />

    <TextView
        android:id="@+id/tv_gender2"
        style="@style/FragmentTextView.Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_data"
        app:layout_constraintBottom_toTopOf="@+id/tv_province2"
        app:layout_constraintEnd_toEndOf="@+id/tv_province2"
        app:layout_constraintStart_toEndOf="@+id/tv_gender"
        app:layout_constraintTop_toBottomOf="@+id/tv_dob2" />

    <TextView
        android:id="@+id/tv_province2"
        style="@style/FragmentTextView.Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_data"
        app:layout_constraintBottom_toTopOf="@+id/tv_civil2"
        app:layout_constraintEnd_toEndOf="@+id/tv_civil2"
        app:layout_constraintStart_toEndOf="@+id/tv_province"
        app:layout_constraintTop_toBottomOf="@+id/tv_gender2" />

    <TextView
        android:id="@+id/tv_civil2"
        style="@style/FragmentTextView.Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/tv_data"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_civil"
        app:layout_constraintTop_toBottomOf="@+id/tv_province2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintGuide_percent="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>